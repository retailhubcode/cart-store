{
  "name": "@shared-libs/cart-store",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retailhubcode/cart-store.git"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --format=esm --target=esnext --platform=browser --jsx=automatic --define:process.env.NODE_ENV='\"production\"' --define:process.env.NEXT_PUBLIC_IS_CMS='false' --minify=false --outfile=dist/index.js --sourcemap",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "esbuild": "^0.18.18",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@nanostores/react": "^1.0.0",
    "nanostores": "^1.0.1"
  }
}
